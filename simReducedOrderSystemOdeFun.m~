function [ eta_dot ] = simReducedOrderSystemOdeFun( t, eta, k, a, c, d, ...
                                                    e, k_tan,           ...
                                                    b_p_c, b_p_v,       ...
                                                    b_c_c, b_c_v        )
  xi = -k*eta;
  eta2 = eta(2);
  eta3 = eta(3);
  
eta_dot = -(eta2 - a*xi)/(c*cos(eta3))                                    ;
           e*sin(eta3) - b_p_v*xi                                       ...
           + b_p_c*tanh((k_tan*(eta2 - a*xi))/(c*cos(eta3))) ...
           + c*xi*sin(eta3);
                                                                       xi];



% [-(eta2 - a*xi)/(c*cos(eta3))                                             ;
%   c*xi*sin(eta3) +                                                      ...
%   (a*(b_c_v*(eta2 - a*xi) + d*e*sin(eta3) +                             ...
%   b_p_c*d*tanh((k_tan*(eta2 - a*xi))/(c*cos(eta3))) - b_p_v*d*xi -      ...
%   (sin(eta3)*(eta2 - a*xi)^2)/cos(eta3) -                               ...
%   b_c_c*c*tanh(k_tan*xi)*cos(eta3)))/(a*d - c^2*cos(eta3)^2) +          ...
%   (c*cos(eta3)*(a*b_c_c*tanh(k_tan*xi) -                                ...
%   b_p_c*c*tanh((k_tan*(eta2 - a*xi))/(c*cos(eta3)))*cos(eta3) +         ...
%   b_p_v*c*xi*cos(eta3) - c*e*cos(eta3)*sin(eta3) -                      ...
%   (a*b_c_v*(eta2 - a*xi))/(c*cos(eta3)) +                               ...
%   (a*sin(eta3)*(eta2 - a*xi)^2)/(c*cos(eta3)^2)))/(a*d - c^2*cos(eta3)^2) ;
%   xi                                                                     ];
  
  %eta_dot_dot = eta_dot(3);
  
end